<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:id="@+id/channel_order_wizzard_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="返回"
        android:textColor="#ffffffff"
        android:textSize="48sp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="32dp"
        android:focusable="true"/>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_gravity="center"
        android:gravity="center">
        <TextView
            android:id="@+id/channel_order_wizzard_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="产品订购"
            android:textColor="#ffffffff"
            android:textSize="72sp"
            android:textStyle="bold"/>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <Button
                android:id="@+id/channel_order_wizzard_btn_primary"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="9.00元/月"/>
            <Button
                android:id="@+id/channel_order_wizzard_btn_secondary"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="19.00元/月"/>
        </LinearLayout>
    </LinearLayout>
</FrameLayout>